

import java.util.*;


public class V91_Arraylist_Demo {
public static void main(String[]args) {
	
	ArrayList<Integer> l1 =new ArrayList<>(); 	
	l1.add(6);
	l1.add(7);
	l1.add(4);
	l1.add(6);
	l1.add(5);
	l1.add(0,5);
	l1.add(0,6);
	
	for(int i=0; i<l1.size(); i++) {
		System.out.println(l1.get(i));    /*op=   6
		                                          5
													6
													7
													4
													6
													5 */
		
		

	}
	
}
	
	
	
	
	
	
	
}
