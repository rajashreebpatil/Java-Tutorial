

 class Two extends One{
	@Override
	 public void greet() {
	 System.out.println("good  morning");
	}
	
@Deprecated
	public int  sum(int a, int b) {
	return a+B;
		
}
 }
	 
	 






public class V108_Annotation {
public static void main(String[] args) {
	
	Two c = new Two();
	
	c.greet();
	c.sum(5, 4);
	
	
}
}
