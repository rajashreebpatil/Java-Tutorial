import java.io.File;
import java.io.IOException;

public class V111_File_handling { 
	public static void main(String[] args) {
		
		File my= new  File("V111Filehandling.txt");
		
		try {
			my.createNewFile();
		} catch (IOException e) {
			System.out.println("unable to create file");
			e.printStackTrace();
		}
		
		
		
		
		
		
	}
	
}
